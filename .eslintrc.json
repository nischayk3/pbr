{
	"env": {
		// "browser": true,
		"node": true
	},
	"settings": {
		"react": {
			"version": "autoDetect"
		}
	},
	"extends": [
		"eslint:recommended"
		//  "plugin:react/recommended",
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 6,
		"sourceType": "module"
	},

	"rules": {
		"no-unused-vars": "off",
		"indent": ["off", 2],
		"linebreak-style": ["off", "windows"],
		"quotes": ["off", "single"],
		"semi": ["off", "always"],
		"no-mixed-spaces-and-tabs": 0,
		// "no-const-assign":"error",
		// "no-dupe-args":"error",
		// "no-dupe-else-if":"error",
		// "no-dupe-keys":"error",
		// "no-duplicate-case":"error",
		// "no-duplicate-imports":"error",
		// "no-empty-pattern":"error",
		// "no-irregular-whitespace":"error",
		"no-console": "warn"
		// "import/order": [
		//     "error",
		//     {
		//       "groups": ["builtin", "external", "internal"],
		//       "pathGroups": [
		//         {
		//           "pattern": "react",
		//           "group": "external",
		//           "position": "before"
		//         }
		//       ],
		//       "pathGroupsExcludedImportTypes": ["react"],

		//       "alphabetize": {
		//         "order": "asc",
		//         "caseInsensitive": true
		//       }
		//     }
		//   ]
	}
}
